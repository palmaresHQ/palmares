{
  "name": "@palmares/drizzle-engine",
  "version": "0.0.1",
  "description": "This is the engine that wraps the hole drizzle interface in a way palmares can understand and have full control of",
  "main": "./dist/cjs/src/index.js",
  "module": "./dist/esm/src/index.js",
  "types": "./dist/cjs/types/index.d.ts",
  "exports": {
    "./better-sqlite3":{
      "types": "./dist/cjs/types/drizzles/better-sqlite3.d.ts",
      "node": "./dist/cjs/src/drizzles/better-sqlite3.js",
      "require": "./dist/cjs/src/drizzles/better-sqlite3.js",
      "import": "./dist/esm/src/drizzles/better-sqlite3.js",
      "default": "./dist/cjs/src/drizzles/better-sqlite3.js"
    },
    "./aws-data-api/pg":{
      "types": "./dist/cjs/types/drizzles/aws-data-api/pg.d.ts",
      "node": "./dist/cjs/src/drizzles/aws-data-api/pg.js",
      "require": "./dist/cjs/src/drizzles/aws-data-api/pg.js",
      "import": "./dist/esm/src/drizzles/aws-data-api/pg.js",
      "default": "./dist/cjs/src/drizzles/aws-data-api/pg.js"
    },
    "./bun-sqlite":{
      "types": "./dist/cjs/types/drizzles/bun-sqlite.d.ts",
      "node": "./dist/cjs/src/drizzles/bun-sqlite.js",
      "require": "./dist/cjs/src/drizzles/bun-sqlite.js",
      "import": "./dist/esm/src/drizzles/bun-sqlite.js",
      "default": "./dist/cjs/src/drizzles/bun-sqlite.js"
    },
    "./d1":{
      "types": "./dist/cjs/types/drizzles/d1.d.ts",
      "node": "./dist/cjs/src/drizzles/d1.js",
      "require": "./dist/cjs/src/drizzles/d1.js",
      "import": "./dist/esm/src/drizzles/d1.js",
      "default": "./dist/cjs/src/drizzles/d1.js"
    },
    "./expo-sqlite":{
      "types": "./dist/cjs/types/drizzles/expo-sqlite.d.ts",
      "node": "./dist/cjs/src/drizzles/expo-sqlite.js",
      "require": "./dist/cjs/src/drizzles/expo-sqlite.js",
      "import": "./dist/esm/src/drizzles/expo-sqlite.js",
      "default": "./dist/cjs/src/drizzles/expo-sqlite.js"
    },
    "./libsql":{
      "types": "./dist/cjs/types/drizzles/libsql.d.ts",
      "node": "./dist/cjs/src/drizzles/libsql.js",
      "require": "./dist/cjs/src/drizzles/libsql.js",
      "import": "./dist/esm/src/drizzles/libsql.js",
      "default": "./dist/cjs/src/drizzles/libsql.js"
    },
    "./neon-http":{
      "types": "./dist/cjs/types/drizzles/d1.d.ts",
      "node": "./dist/cjs/src/drizzles/d1.js",
      "require": "./dist/cjs/src/drizzles/d1.js",
      "import": "./dist/esm/src/drizzles/d1.js",
      "default": "./dist/cjs/src/drizzles/d1.js"
    },
    "./node-postgres":{
      "types": "./dist/cjs/types/drizzles/node-postgres.d.ts",
      "node": "./dist/cjs/src/drizzles/node-postgres.js",
      "require": "./dist/cjs/src/drizzles/node-postgres.js",
      "import": "./dist/esm/src/drizzles/node-postgres.js",
      "default": "./dist/cjs/src/drizzles/node-postgres.js"
    },
    "./op-sqlite":{
      "types": "./dist/cjs/types/drizzles/op-sqlite.d.ts",
      "node": "./dist/cjs/src/drizzles/op-sqlite.js",
      "require": "./dist/cjs/src/drizzles/op-sqlite.js",
      "import": "./dist/esm/src/drizzles/op-sqlite.js",
      "default": "./dist/cjs/src/drizzles/op-sqlite.js"
    },
    "./pg-proxy":{
      "types": "./dist/cjs/types/drizzles/pg-proxy.d.ts",
      "node": "./dist/cjs/src/drizzles/pg-proxy.js",
      "require": "./dist/cjs/src/drizzles/pg-proxy.js",
      "import": "./dist/esm/src/drizzles/pg-proxy.js",
      "default": "./dist/cjs/src/drizzles/d1pg-proxy.js"
    },
    "./pglite":{
      "types": "./dist/cjs/types/drizzles/pglite.d.ts",
      "node": "./dist/cjs/src/drizzles/pglite.js",
      "require": "./dist/cjs/src/drizzles/pglite.js",
      "import": "./dist/esm/src/drizzles/pglite.js",
      "default": "./dist/cjs/src/drizzles/pglite.js"
    },
    "./postgres-js":{
      "types": "./dist/cjs/types/drizzles/postgres-js.d.ts",
      "node": "./dist/cjs/src/drizzles/postgres-js.js",
      "require": "./dist/cjs/src/drizzles/postgres-js.js",
      "import": "./dist/esm/src/drizzles/postgres-js.js",
      "default": "./dist/cjs/src/drizzles/postgres-js.js"
    },
    "./vercel-postgres":{
      "types": "./dist/cjs/types/drizzles/vercel-postgres.d.ts",
      "node": "./dist/cjs/src/drizzles/vercel-postgres.js",
      "require": "./dist/cjs/src/drizzles/vercel-postgres.js",
      "import": "./dist/esm/src/drizzles/vercel-postgres.js",
      "default": "./dist/cjs/src/drizzles/vercel-postgres.js"
    },
    "./xata-http":{
      "types": "./dist/cjs/types/drizzles/xata-http.d.ts",
      "node": "./dist/cjs/src/drizzles/xata-http.js",
      "require": "./dist/cjs/src/drizzles/xata-http.js",
      "import": "./dist/esm/src/drizzles/xata-http.js",
      "default": "./dist/cjs/src/drizzles/xata-http.js"
    },
    ".": {
      "types": "./dist/cjs/types/index.d.ts",
      "node": "./dist/cjs/src/index.js",
      "require": "./dist/cjs/src/index.js",
      "import": "./dist/esm/src/index.js",
      "default": "./dist/esm/src/index.js"
    }
  },
  "scripts": {
    "clear": "rimraf ./dist",
    "build:types": "tsc --project tsconfig.types.json",
    "build:cjs": "swc ./src -d ./dist/cjs --config-file ../../build/.commonjs.swcrc",
    "build:module": "swc ./src -d ./dist/esm --config-file ../../build/.esm.swcrc",
    "build": "pnpm run clear && pnpm run build:cjs && pnpm run build:module && pnpm run build:types",
    "build:types:watch": "tsc --project tsconfig.types.json --watch --preserveWatchOutput",
    "build:cjs:watch": "swc ./src -d ./dist/cjs --config-file ../../build/.commonjs.swcrc --watch",
    "build:module:watch": "swc ./src -d ./dist/esm --config-file ../../build/.esm.swcrc --watch",
    "build:watch": "pnpm run build:cjs:watch & pnpm run build:module:watch & pnpm run build:types:watch"
  },
  "keywords": [
    "drizzle",
    "drizzle-engine",
    "palmares"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/palmaresHQ/palmares.git"
  },
  "bugs": {
    "url": "https://github.com/palmaresHQ/palmares/issues"
  },
  "homepage": "https://github.com/palmaresHQ/palmares#readme",
  "author": "Nicolas Melo",
  "license": "MIT",
  "dependencies": {
    "@palmares/core": "workspace:*",
    "@palmares/databases": "workspace:*",
    "drizzle-orm": "^0.32.1"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.11"
  }
}
